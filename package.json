{
  "name": "slack-potluck",
  "version": "0.5.4-alpha",
  "description": "Slack Potluck app",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "db:init": "npm run db:create && npm run db:migrate",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/_mocha --config .mocharc.js test/"
  },
  "keywords": [
    "slack"
  ],
  "author": "EPICmynamesBG@users.noreply.github.com",
  "license": "ISC",
  "dependencies": {
    "@slack/bolt": "^3.12.2",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.42",
    "mysql2": "^3.2.0",
    "sequelize": "^6.29.3",
    "sqlite3": "^5.1.6",
    "winston": "^3.8.2"
  },
  "engines": {
    "node": "18.5.0",
    "npm": "9.6.1"
  },
  "devDependencies": {
    "mocha": "^10.2.0",
    "sequelize-cli": "^6.6.0",
    "sinon": "^15.2.0",
    "terminate": "^2.6.1"
  }
}
